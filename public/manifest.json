{
  "manifest_version": 2,

  "name": "Gar's Study Tab",
  "description": "A simple new tab extension.",
  "version": "1.0",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "browser_action": {
    "default_icon": "favicon.png"
  },

  "permissions": [
    "tabs",
    "bookmarks",
    "history"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
